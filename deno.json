{
  "name": "@ethandev512/lightbox-gallery",
  "version": "0.1.2",
  "description": "A modern, customizable image lightbox web component built with FAST Element and Splide.js.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamtanloc512/lightbox-gallery.git"
  },
  "license": "MIT",
  "exports": "./mod.ts",
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview"
  },
  "compilerOptions": {
    "lib": ["ESNEXT", "DOM", "DOM.Iterable"],
    "checkJs": true,
    "experimentalDecorators": true
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.6",
    "@fluentui/web-components": "npm:@fluentui/web-components@^2.6.1",
    "@microsoft/fast-element": "npm:@microsoft/fast-element@^2.9.1",
    "@splidejs/splide": "npm:@splidejs/splide@^4.1.4",
    "lodash": "npm:lodash@^4.17.21",
    "normalize.css": "npm:normalize.css@^8.0.1",
    "vite": "npm:vite@^6.4.1"
  }
}
